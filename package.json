{
  "name": "kanji-assembly",
  "version": "0.1.0",
  "private": true,
  "description": "web site of the Kanji assembly puzzle",
  "author": "tositeru",
  "scripts": {
    "build": "DOMAIN=\"www.kanji-assembly.site\" nuxt build",
    "start-test-server": "cross-env NODE_ENV=test nodemon server/index.js --watch server --watch db",
    "test": "cross-env NODE_ENV=test node test-oreore/user.test.js",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore --ignore-pattern Q/insertKanji.js .",
    "dev": "cross-env NODE_ENV=development USE_TEST_MAIL=1 UNLIMIT_QUESTION_DATE=1 nodemon server/index.js --watch server --watch db",
    "dev:mail": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch db",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "start": "cross-env DOMAIN=\"www.kanji-assembly.site\" NODE_ENV=production HOST=\"0.0.0.0\" PORT=443 node server/index.js",
    "test:update-snapshots": "ava --update-snapshots",
    "open-database": "sqlite3 db/database.db",
    "db:create-model": "node_modules/.bin/sequelize model:create",
    "db:create-migration": "node_modules/.bin/sequelize migration:create",
    "db:remake-seed": "cross-env NODE_ENV=development node_modules/.bin/sequelize db:seed:undo:all && cross-env NODE_ENV=development node_modules/.bin/sequelize db:seed:all",
    "addQ": "node Q/createQ"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/bulma": "^1.2.1",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/vuetify": "^0.5.5",
    "body-parser": "^1.18.3",
    "consola": "^2.5.6",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.0",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "nodemailer": "^5.1.1",
    "nuxt": "^2.4.0",
    "path": "^0.12.7",
    "pug": "^2.0.3",
    "redirect-ssl": "^1.4.0",
    "sequelize": "^5.2.1",
    "sqlite3": "^4.0.6",
    "striptags": "^3.1.1",
    "twitter": "^1.7.1",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^8.2.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-env": "^3.2.5",
    "command-line-args": "^5.1.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "jsdom": "^13.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "prettier": "1.14.3",
    "pug-plain-loader": "^1.0.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^2.0.0",
    "sass-loader": "^7.1.0",
    "sequelize-cli": "^5.4.0",
    "shelljs": "^0.8.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-eslint-parser": "^6.0.2",
    "vuetify-loader": "^1.0.5"
  }
}
